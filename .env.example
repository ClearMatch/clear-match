# Clear Match Environment Configuration
# Copy this file to .env.local and fill in your values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# External API Keys
HUBSPOT_API_KEY=your-hubspot-api-key-here

# Security Configuration
SESSION_SECRET=your-32-character-session-secret-here-abcdef0123456789
NEXT_PUBLIC_APP_URL=https://your-app-domain.com

# Rate Limiting (Optional - Redis for production)
# If not provided, will use memory-based rate limiting
UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-redis-token-here

# Node Environment
NODE_ENV=development